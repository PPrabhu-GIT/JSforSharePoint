//Hide Multi Line Text Box Column In SharePoint Edit Form Using JQuery
<script src="https://ajax.googleapis.com/ajax/libs/jquery/1.3.1/jquery.min.js"
       type="text/javascript"></script>
<script type="text/javascript">
$(function() {
//Hide Multiple line text box field
  $("textarea[title='Your field display name']").closest('tr').hide();
}); 
</script>
